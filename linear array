#include<stdio.h>
int main()
{
	int arr[30],n,i,s;
	printf("enter the size of element");
	scanf("%d",&n);
	printf("enter the array element:");
	for(i=0;i<n;i++)
	scanf("%d",&arr[i]);
	printf("enter the search element");
	scanf("%d",&s);
	for(i=0;i<n;i++)
	{
		if(s==arr[i])
		{
			printf("enter the searched element are founded in %d inde
		}
		if(i==n)
		{
			printf("the search element are not found");
		}
	}
	return 0;
}
